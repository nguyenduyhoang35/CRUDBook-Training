<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8"/>
    <title>Title</title>
    <script>
        function makeGetRequest() {
            let request = new XMLHttpRequest();
            request.onreadystatechange = function () {
                if (this.readyState === 4 && this.status === 200) {
                    document.getElementById('test').innerHTML = request.responseText;
                }
            };
            request.open('get', '/books', false);
            request.send();
        }
    </script>
</head>
<body>
    <header>

    </header>
    <nav>

    </nav>
    <section>
        <div id="test"></div>
        <button onclick="makeGetRequest()">Test</button>
    </section>
</body>
</html>